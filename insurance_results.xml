<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white"
    tools:context=".InsuranceResults"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <RelativeLayout
        android:background="#0012BD"
        android:layout_width="match_parent"
        android:layout_height="120dp">
        <ImageView
            android:id="@+id/arrow_back22"
            android:layout_width="40dp"
            android:layout_height="70dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            app:tint="@color/white"
            android:contentDescription="Back Arrow"
            android:src="@drawable/arrow_back" />

        <TextView
            android:id="@+id/textView111"
            android:layout_width="400dp"
            android:layout_marginLeft="70dp"
            android:layout_height="100dp"
            android:layout_marginTop="35dp"
            android:background="#0012BD"
            android:text="Available Insurance"
            android:textColor="@color/white"
            android:textSize="30sp"
            android:textStyle="bold"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="10dp"
        android:layout_width="380dp"
        android:layout_height="250dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="340dp"
            android:layout_height="227dp"
            android:layout_centerHorizontal="true"
            app:srcCompat="@drawable/sample_textbox"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="120dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView16"
            android:layout_marginTop="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Shaheen Insuranace"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:layout_centerHorizontal="true"
            tools:ignore="MissingConstraints"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="75dp"
            android:text="Rate"
            android:textColor="@color/black"
            android:textSize="22sp"
            />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="235dp"
            android:layout_marginTop="75dp"
            android:text="1.45%"
            android:textColor="@color/black"
            android:textSize="20sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="140dp"
            android:text="Total"
            android:textColor="@color/black"
            android:textSize="22sp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="235dp"
            android:layout_marginTop="140dp"
            android:text="2900/year"
            android:textColor="@color/black"
            android:textSize="20sp"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="10dp"
        android:layout_width="380dp"
        android:layout_height="250dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="340dp"
            android:layout_height="227dp"
            android:layout_centerHorizontal="true"
            app:srcCompat="@drawable/sample_textbox"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="120dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView32"
            android:layout_marginTop="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pak Turk Marif"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:layout_centerHorizontal="true"
            tools:ignore="MissingConstraints"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="75dp"
            android:text="Rate"
            android:textColor="@color/black"
            android:textSize="22sp"
            />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="235dp"
            android:layout_marginTop="75dp"
            android:text="3.5%"
            android:textColor="@color/black"
            android:textSize="20sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="45dp"
            android:layout_marginTop="140dp"
            android:text="Total"
            android:textColor="@color/black"
            android:textSize="22sp"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="235dp"
            android:layout_marginTop="140dp"
            android:text="3000/year"
            android:textColor="@color/black"
            android:textSize="20sp"
            />
    </RelativeLayout>

</LinearLayout>